<a name="pagecontent" id="pagecontent"></a>

<div class="page page--block-text">
  
  <div class="container">
    <div class="row">

      <div class="sixteen columns">
        <div class="page-heading">
          <h1 class="page-heading__title">
            {{ page.title }}
          </h1>
        </div>

        <div>
          {% render 'page-multi-column', content: page.content %}
        </div>
      </div>

    </div>
  </div>
  
</div>